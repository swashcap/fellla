{
  "name": "fellla",
  "version": "0.0.1",
  "description": "Experimentation with Fela",
  "repository": "https://github.com/swashcap/fellla",
  "author": "Cory Reed",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.7.5",
    "fela": "^11.0.2",
    "fela-bindings": "^11.0.2",
    "preact": "^10.0.5",
    "preact-fela": "^11.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.7.5",
    "@babel/plugin-transform-typescript": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-alpha.3.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "build": "run-s build:ts build:parcel",
    "build:parcel": "parcel build src/index.html",
    "build:ts": "tsc --noEmit",
    "clean": "./scripts/clean.sh",
    "format": "prettier --write '*.js' '{src,types}/**/*.{html,ts,tsx}'",
    "start": "parcel src/index.html"
  },
  "alias": {
    "preact-context": "preact"
  }
}
